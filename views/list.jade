extends layout

block content
  .container
    .row
      .col-sm-8
        .section-header
          h3 About #{meta.totalResults} results
          .section-nav
            ul.sorting
              li.sort-text Sort by:
              li
                a(href='/?s=music&order_post=latest') Latest
              li
                a(href='/?s=music&order_post=viewed') Viewed
              li
                a(href='/?s=music&order_post=liked') Liked
              li
                a(href='/?s=music&order_post=comments') Comments
        .row.video-section.meta-maxwidth-230
          each vid in vids
            .col-sm-4.col-xs-6.item
              a(title='#{vid.snippet.title}', href='/#{vid.snippet.slug}/#{vid.id.videoId}')
                img.img-responsive.wp-post-image(width='230', height='150', src='#{vid.snippet.thumbnails.high.url}', alt='#{vid.snippet.title}')
              h3
                a(title='#{vid.snippet.title}', href='/#{vid.snippet.slug}/#{vid.id.videoId}')
                  | #{vid.snippet.title}
              .meta
                span.date 1 year ago
                span.views
                  i.fa.fa-eye
                  | 7356
                span.heart
                  i.fa.fa-heart
                  | 825
                span.fcomments
                  i.fa.fa-comments
                  | 6

          .col-sm-12
            span.meta-info Related keywords
              if relatedKeywords
                each keyword in relatedKeywords
                  a(href='/#{keyword}', rel='keyword') #{keyword}

      .col-sm-4.sidebar
        .widget.mars-subscribox-widget
          h4.widget-title Social Subscribox
          .social-counter-item
            a(target='_top', href='#')
              i.fa.fa-facebook
              span.counter 129
              span.counter-text Fans
          .social-counter-item
            a(target='_top', href='#')
              i.fa.fa-twitter
              span.counter 126
              span.counter-text Followers
          .social-counter-item
            a(target='_top', href='#')
              i.fa.fa-google-plus
              span.counter 235
              span.counter-text Fans
          .social-counter-item.last.subscribe
            a(href='#', data-toggle='modal', data-target='#subscrib-modal')
              i.fa.fa-rss
              span.counter 2
              span.counter-text Subscribers
          // Modal
          #subscrib-modal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='subscrib-modal-label', aria-hidden='true')
            .modal-dialog
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal', aria-hidden='true') Ã—
                  h4#subscrib-modal-label.modal-title Subscribe
                .modal-body
                  form#mars-subscribe-form(method='post', role='form', action='', name='mars-subscribe-form')
                    .form-group.name
                      label(for='name') Your Name
                      input#name.form-control(type='text')
                    .form-group.email
                      label(for='email') Your Email Address
                      input#email.form-control(type='email')
                    .checkbox
                      label
                        input#agree(name='agree', type='checkbox')
                        a(href='http://videotube.marstheme.com/video/wonderfull-chill-out-music-love/') User Agreement & Privacy Policy
                    input#mars_subscrib(type='hidden', name='mars_subscrib', value='992bf4a282')
                    input(type='hidden', name='_wp_http_referer', value='/?s=music')
                    button.btn.btn-primary(type='submit') Register
                    input(type='hidden', name='submit-label', value='Register')
                    button.btn.btn-default(type='button', data-dismiss='modal') Close
                    input#referer(type='hidden', name='referer', value='1903')
